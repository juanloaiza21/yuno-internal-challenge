{
  "functions": {
    "api/**/*.rs": {
      "runtime": "vercel-community/rust@latest"
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/authorize",
      "destination": "/api/authorize"
    },
    {
      "source": "/api/report",
      "destination": "/api/report"
    }
  ]
}
